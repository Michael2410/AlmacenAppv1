# Frontend environment for AlmacenApp

# API base URL. In dev, prefer leaving this unset to use Vite proxy on /api
 VITE_API_URL=http://localhost:3001/api

# Use local mocks (true/false). Set to false to use real backend.
VITE_USE_MOCKS=false

# Dev proxy target (only used by Vite dev server). Example backend:
VITE_PROXY_TARGET=http://localhost:3001
